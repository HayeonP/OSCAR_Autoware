<?xml version="1.0"?>
<launch>
  <node pkg="teleop_twist_keyboard" type="teleop_twist_keyboard.py" name="teleop_twist_keyboard" output="screen" />
  <node pkg="control_module" type="twist_converter" name="twist_converter"/>
  <rosparam>
    speed_to_erpm_gain: 4614
    speed_to_erpm_offset: 0.0
    steering_angle_to_servo_gain: -1.2135
    steering_angle_to_servo_offset: 0.5304
    wheelbase: .25
    vesc_to_odom/publish_tf: false
    vesc_driver/port: /dev/vesc
  </rosparam>
  <node pkg="vesc_driver" type="vesc_driver_node" name="vesc_driver"/>
  <node pkg="control_module" type="control_module" name="control_module">
    <param name="keyboard" value="true" />
  </node>
</launch>
